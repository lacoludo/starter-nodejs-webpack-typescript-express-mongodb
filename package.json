{
  "name": "starter-nodejs-webpack-typescript-express-mongodb",
  "version": "1.0.0",
  "author": "Ludovic Lacouture <lacoludo@outlook.com>",
  "scripts": {
    "start": "tsnd --respawn src/server.ts",
    "fmt": "prettier --write \"**/*.{js,json,md,ts}\"",
    "bundle": "webpack --config webpack.config.js"
