  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.0",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.7",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5",
